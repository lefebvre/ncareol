# Single file that needs to be changed on a release branch
# or on the development branch in order to configure ncareol
# for release mode and set the version.

SET(ncareol_NAME "ncareol")
SET(ncareol_VERSION 0.1)
SET(ncareol_FULL_VERSION "0.1")
SET(ncareol_VERSION_STRING "0.1 (aplha)")
SET(ncareol_MAJOR_VERSION 00)
SET(ncareol_MAJOR_MINOR_VERSION 001000)
SET(ncareol_ENABLE_DEVELOPMENT_MODE_DEFAULT ON) # Change to 'OFF' for a release (beta or final)

